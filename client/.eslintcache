ds, defaultData)\r\n            .then(res => {\r\n                dispatch(actions.diasbleEditModal());\r\n                return res\r\n            });\r\n    }\r\n\r\n    const onOpenHandler = () => {\r\n        setEditFields()\r\n        Server.fetchTableDesc(tableName).then(response => {\r\n            setTableDesc(response.data);\r\n        })\r\n    }\r\n\r\n    const onCloseHandler = (e) => {\r\n        e.preventDefault();\r\n        dispatch(actions.diasbleEditModal());\r\n    }\r\n\r\n\r\n    let modalContent = (\r\n        <form onSubmit={(e) => saveChanges(e)}>\r\n            <table style={{ width: \"100%\" }}>\r\n                <thead>\r\n                    <tr>\r\n                        {\r\n                            tableDesc ?\r\n                                Object.keys(tableDesc[0]).map((columnName, colId) => {\r\n                                    return (\r\n                                        <th key={colId}>\r\n                                            {columnName}\r\n                                        </th>\r\n                                    )\r\n                                }) : null\r\n                        }\r\n                        <th>\r\n                            Value\r\n                        </th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                    {\r\n                        defaultData ?\r\n                            Object.keys(defaultData).map((dataName, dataID) => {\r\n                                return (\r\n                                    tableDesc[dataID] ?\r\n                                        <tr key={dataName}>\r\n                                            {\r\n                                                Object.keys(tableDesc[dataID]).map((descName, descId) => {\r\n                                                    return (\r\n                                                        <td key={descName}>{tableDesc[dataID]