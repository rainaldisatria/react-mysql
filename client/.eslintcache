            logOut();\r\n                    }}>Log Out</MenuItem>\r\n                </Menu>\r\n            </div>\r\n\r\n        logIn = null;\r\n        signUp = null;\r\n    }\r\n    else {\r\n        logIn =\r\n            <Link to='/login'>\r\n                <Button className={classes.white}> Log In </Button>\r\n            </Link>;\r\n        signUp =\r\n            <Link to='/signup'>\r\n                <Button className={classes.white}> Sign Up </Button>\r\n            </Link>;\r\n\r\n        myProfile = null;\r\n    }\r\n\r\n    return (\r\n        <>\r\n            <AppBar position='fixed' className={classes.appBar}>\r\n                <Toolbar>\r\n                    <Link to='/'>\r\n                        <Button className={classes.white} onClick={() => dispatch(searchSync(''))}>\r\n                            <LocalHospitalIcon className={classes.menuButton} />\r\n                            <Typography variant={'h6'}>APOTEK JAKARTA </Typography>\r\n                        </Button>\r\n                    </Link>\r\n\r\n                    <div className={classes.root} />\r\n\r\n                    <div className={classes.search}>\r\n                        <div className={classes.searchIcon}>\r\n                            <SearchIcon />\r\n                        </div>\r\n                        <InputBase\r\n                            fullWidth\r\n                            placeholder=\"Searchâ€¦\"\r\n                            classes={{\r\n                                root: classes.inputRoot,\r\n                                input: classes.inputInput,\r\n                            }}\r\n                            inputProps={{ 'aria-label': 'search' }}\r\n                            autoFocus\r\n                            onKeyDown={search}\r\n                        />\r\n                    </div>\r\n\r\n                    <div className={classes.root} />\r\n                    {admin}\r\n\r\n                    {signUp}\r\n     